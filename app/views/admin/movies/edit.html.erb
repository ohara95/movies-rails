<div class="container ">
  <%= form_for @movie, url: admin_movie_path(@movie) do |f| %>
    <%= render :partial => 'form', :locals => {:f => f} %>
    <div class="d-flex justify-content-center mt-2">
      <%= f.submit t('update'), class: 'btn btn-outline-dark col-md-2' %>
      <%= link_to '戻る', admin_movies_path, class: 'btn btn-outline-dark col-md-2 ml-2' %>
    </div>
  <% end %>
</div>