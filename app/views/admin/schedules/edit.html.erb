<div class="container">
  <%= form_for @schedule, url: admin_schedule_path(@schedule) do |f| %>
    <%= render :partial => 'form', :locals => {:f => f}, model: @schedule %>
    <div class="d-flex justify-content-center mt-2">
      <%= f.submit t('update'), class: 'btn btn-outline-dark col-md-2' %>
      <%= link_to '戻る', admin_movie_schedules_path(params[:id]), class: 'btn btn-outline-dark col-md-2 ml-2' %>
    </div>
  <% end %>
</div>